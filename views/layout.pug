doctype html
html
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/style.css')
    link(rel='stylesheet', href='https://cdn.bootcss.com/font-awesome/5.9.0/css/all.min.css')
    link(rel='stylesheet', href='https://cdn.bootcss.com/twitter-bootstrap/4.3.1/css/bootstrap.min.css')
  body
    nav.navbar.navbar-expand-lg.navbar-light.bg-light
      a.navbar-brand(href='#') #{title}
    block content
    block modals
  script(src='https://cdn.bootcss.com/jquery/3.4.1/jquery.min.js')
  script(src='https://cdn.bootcss.com/twitter-bootstrap/4.3.1/js/bootstrap.min.js')
  script(src='https://cdn.jsdelivr.net/npm/bs-custom-file-input/dist/bs-custom-file-input.js')
  script(src='/javascripts/table.js')
  script.
    $(document).ready(function () {
      bsCustomFileInput.init()
    })
